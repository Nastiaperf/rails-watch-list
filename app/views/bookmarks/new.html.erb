<div class="container">

  <h1>Add a new bookmark</h1>

  <%= simple_form_for [ @list, @bookmark ] do |f| %>
    <%# <%= f.association :list %>
    <%= f.association :movie %>
    <%= f.input :comment %>
    <%= f.submit "Submit bookmark", class: "btn btn-success" %>
  <% end %>

  <%= link_to "Go back to see all lists", lists_path %>
</div>
